{
    "name": "A Flight of Fantasy Dev Environment",
    "image": "mcr.microsoft.com/devcontainers/python:3.9",
    "features": {},
    "postCreateCommand": "pip install -r requirements.txt",
    "customizations": {
      "vscode": {
        "extensions": [
          "ms-python.python",
          "ms-python.vscode-pylance"
        ]
      }
    },
    "ports": [
      {
        "containerPort": 5000,
        "hostPort": 5000,
        "protocol": "http"
      }
    ]
  }